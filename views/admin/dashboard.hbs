<div class="main-panel">
  <div class="content-wrapper">
    <div class="row">
      <div class="col-md-12 grid-margin">
        <div class="row">
          <div class="col-12 col-xl-8 mb-4 mb-xl-0">
            <h3 class="font-weight-bold">Welcome {{user.Username}} !</h3>

          </div>

        </div>
      </div>
    </div>
    <div class="row">

      <div class="col-md-12 grid-margin transparent">
        <div class="row">
          <div class="col-md-6 mb-4 stretch-card transparent">
            <div class="card card-tale">
              <div class="card-body">
                <p class="mb-4">Registered Users</p>
                <p class="fs-30 mb-2">{{usersList.length}}</p>
                {{!-- <p>10.00% (30 days)</p> --}}
              </div>
            </div>
          </div>
          <div class="col-md-6 mb-4 stretch-card transparent">
            <div class="card card-dark-blue">
              <div class="card-body">
                <p class="mb-4">No of courses</p>
                <p class="fs-30 mb-2">{{coursesList.length}}</p>
                {{!-- <p>22.00% (30 days)</p> --}}
              </div>
            </div>
          </div>
        </div>

      </div>
    </div>

    <div class="row">
      <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <div class="row d-flex justify-content-between p-3">
              <div>
                <h4 class="card-title pt-2">Users List</h4>
                <p class="card-description">

                </p>
              </div>
             
            </div>


            <div class="table-responsive">
              <table class="table table-striped">
                <thead>
                  <tr>
                    <th>
                      
                    </th>
                    <th>
                      UID
                    </th>
                    <th>
                      Username
                    </th>
                    <th>
                      Email
                    </th>
                    {{!-- <th>
                      Progress
                    </th> --}}
                    <th>
                      Edit
                    </th>
                    <th>
                      Delete
                    </th>
                  </tr>
                </thead>
                <tbody>
                  {{#each usersList}}
                  <tr id="{{this._id}}">
                    <td class="py-1">
                      <img src="../../images/faces/user.png" alt="image" />
                    </td>
                    <td>
                      {{this.UID}}
                    </td>
                    <td>
                      {{this.Username}}
                    </td>
                    <td>
                      {{this.Email}}
                    </td>
                    {{!-- <td>
                      <div class="progress">
                        <div class="progress-bar bg-success" role="progressbar" style="width: 25%" aria-valuenow="25"
                          aria-valuemin="0" aria-valuemax="100"></div>
                      </div>
                    </td> --}}
                    <td>
                      <a href="/admin/edit-user?id={{this._id}}" class="btn btn-primary p-2 pl-3 pr-3">Edit</a>
                    </td>
                    <td>
                      <a onclick="removeUser('{{this._id}}')" class="btn btn-danger p-2 ">Delete</a>

                    </td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>

  </div>
  <!-- content-wrapper ends -->